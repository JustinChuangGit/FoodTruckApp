{
  "expo": {
    "name": "MNCH",
    "slug": "FoodTruckApp",
    "version": "1.0.16",
    "orientation": "portrait",
    "icon": "./assets/images/appstore.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/appstore.png",
      "resizeMode": "contain",
      "backgroundColor": "#ed8200"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.jc.munch",
      "config": {
        "googleMapsApiKey": "AIzaSyDFzVWsI5ajHKH7i5d5kAX-nsOlEQM8cx4"
      },
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSLocationWhenInUseUsageDescription": "This app needs access to your location to show nearby food trucks.",
      }
    },
    "android": {
      "package": "com.jc.munch",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/appstore.png",
        "backgroundColor": "#ed8200"
      },
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyDsOtvAhUXloEk7tUU58ZnyRx4BdvoAWCs"
        }
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      [
        "@sentry/react-native/expo",
        {
          "url": "https://sentry.io/",
          "project": "javascript-react",
          "organization": "mnch-22"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "googleMapsApiKey": "AIzaSyDFzVWsI5ajHKH7i5d5kAX-nsOlEQM8cx4",
      "eas": {
        "projectId": "85c3be11-c802-41b0-b1e1-d598b22324a8"
      }
    },
    "owner": "justinchuang"
  }
}
